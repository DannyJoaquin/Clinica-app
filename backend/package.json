{
  "name": "clinica-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
  "prisma:generate:sqlite": "prisma generate --schema=prisma/schema.sqlite.prisma",
  "prisma:migrate:sqlite": "powershell -Command \"$env:DATABASE_URL='file:./dev.db'; prisma migrate dev --name init --schema=prisma/schema.sqlite.prisma\"",
  "db:reset:sqlite": "powershell -Command \"Remove-Item -ErrorAction SilentlyContinue prisma/dev.db; $env:DATABASE_URL='file:./dev.db'; prisma migrate reset --force --skip-seed --schema=prisma/schema.sqlite.prisma\"",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/seed.ts",
    "e2e": "node scripts/e2e.mjs",
    "e2e:ps": "powershell -ExecutionPolicy Bypass -File scripts/e2e.ps1"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.20",
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
  "pdfkit": "^0.15.0",
  "@nestjs/swagger": "^7.4.2",
  "swagger-ui-express": "^5.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@types/passport-jwt": "^3.0.13",
    "@types/pdfkit": "^0.17.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "^20.0.0 || ^18.0.0"
  }
}
